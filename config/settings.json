{
  "data": {
    "cache_dir": "data/cache",
    "cache_expiry_hours": 24,
    "api_timeout": 30,
    "max_retries": 3,
    "retry_delay": 1.0,
    "network": {
      "use_proxy": false,
      "proxy_host": "",
      "proxy_port": "",
      "proxy_username": "",
      "proxy_password": "",
      "disable_ssl_verify": false,
      "user_agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
    },
    "data_sources": {
      "use_multi_source": true,
      "akshare": {
        "enabled": true,
        "priority": 1,
        "timeout": 30,
        "max_retries": 3
      },
      "tushare": {
        "enabled": true,
        "priority": 2,
        "timeout": 30,
        "max_retries": 3,
        "token": "your_tushare_token_here",
        "proxy_url": "http://your-proxy-url-here"
      }
    }
  },
  "indicators": {
    "ma_periods": [
      5,
      20,
      30
    ],
    "rsi_period": 14,
    "rsi_overbought": 70.0,
    "rsi_oversold": 30.0,
    "rsi_neutral": 50.0
  },
  "signals": {
    "max_drawdown_threshold": 0.2,
    "trend_strength_threshold": 0.6,
    "confidence_threshold": 0.5,
    "enable_trend_filter": true,
    "enable_rsi_filter": true,
    "enable_drawdown_filter": true
  },
  "portfolio": {
    "default_rebalance_threshold": 0.05,
    "config_file": "data/portfolio_config.json",
    "auto_save": true,
    "max_positions": 20
  },
  "ui": {
