# ETF投资仪表盘环境变量配置示例
# 复制此文件为 .env 并根据需要修改配置

# 数据配置
ETF_CACHE_DIR=data/cache
ETF_CACHE_EXPIRY_HOURS=24

# 日志配置
ETF_LOG_LEVEL=INFO
ETF_LOG_FILE=logs/etf_dashboard.log

# 调试模式
ETF_DEBUG=false

# Streamlit配置
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_ADDRESS=localhost

# API配置
ETF_API_TIMEOUT=30
ETF_MAX_RETRIES=3